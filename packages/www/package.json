{
  "name": "@jump-example/www",
  "description": "Jump Example WWW",
  "version": "3.1.1",
  "private": true,
  "license": "UNLICENSED",
  "main": "index.js",
  "dependencies": {
    "@apollo/client": "3.2.5",
    "@hello10/classie": "1.1.1",
    "@hello10/jump-client": "3.1.1",
    "@hello10/jump-util": "3.1.1",
    "@hello10/logger": "0.0.8",
    "babel-polyfill": "6.26.0",
    "email-regex": "4.0.0",
    "firebase": "7.24.0",
    "graphql": "14.2.1",
    "graphql-tag": "2.11.0",
    "jsonp": "0.2.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.2.0",
    "styled-reset": "4.2.3"
  },
  "devDependencies": {
    "@hello10/eslint-config-react": "1.0.2",
    "eslint-plugin-react": "7.21.5",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-static-files-copy": "2.5.0"
  },
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html --public-url ./ --target browser --detailed-report",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f yarn.lock && rm -rf .cache",
    "deploy": "yarn build && yarn firebase deploy --only hosting",
    "lint": "eslint ./src",
    "start": "parcel src/index.html --open"
  },
  "staticFiles": {
    "staticPath": "src/static",
    "watcherGlob": "**"
  },
  "browserslist": [
    "defaults"
  ]
}
