{
  "name": "@jump-example/www",
  "description": "Jump WWW",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "index.js",
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@hello10/classie": "1.1.1",
    "@hello10/jump-client": "2.0.5",
    "@hello10/jump-util": "2.0.5",
    "@hello10/logger": "0.0.8",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link-context": "1.0.20",
    "apollo-link-http": "1.5.17",
    "babel-polyfill": "6.26.0",
    "email-regex": "4.0.0",
    "firebase": "7.16.1",
    "graphql": "^14.0.0 || ^15.0.0",
    "graphql-tag": "2.10.3",
    "jsonp": "0.2.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.0.0",
    "styled-reset": "4.1.0"
  },
  "devDependencies": {
    "@hello10/eslint-config-react": "1.0.2",
    "eslint-plugin-react": "7.19.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-static-files-copy": "2.4.3"
  },
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html --public-url ./ --target browser --detailed-report",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f yarn.lock && rm -rf .cache",
    "deploy": "yarn build && yarn firebase deploy --only hosting",
    "lint": "eslint ./src",
    "start": "parcel src/index.html --open"
  },
  "staticFiles": {
    "staticPath": "src/static",
    "watcherGlob": "**"
  },
  "browserslist": [
    "defaults"
